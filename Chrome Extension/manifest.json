{
	"manifest_version": 2,
	"name": "My Cool Extension",
	"version": "0.1",
	"background": {
    	"scripts": ["background.js"]
	},
	"content_scripts": [
		{
		"matches": ["<all_urls>"],
		"css": [],
		"js": ["jquery-3.4.1.js", "content.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
